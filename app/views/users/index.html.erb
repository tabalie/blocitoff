<div class="row">
  <div class="col-md-8">
    <% @users.each do |user| %>
      <div class="media">
          <div class="media-body">
            <h4 class="media-heading">
              <%= user.name %>
              <%= render user.items %>
            </h4>
          </div>
      </div>
    <% end %>
  </div>
</div>
